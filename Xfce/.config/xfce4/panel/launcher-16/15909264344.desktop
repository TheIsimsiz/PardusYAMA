[Desktop Entry]
Version=1.0
Type=Application
Exec=exo-open --launch MailReader %u
Icon=internet-mail
StartupNotify=true
Terminal=false
Categories=Network;X-XFCE;X-Xfce-Toplevel;
OnlyShowIn=XFCE;
X-XFCE-MimeType=x-scheme-handler/mailto;
X-AppStream-Ignore=True
Name=Mail Reader
Name[tr]=Eposta Okuyucusu
Comment=Read your email
Comment[tr]=E-postanızı okuyun
X-XFCE-Source=file:///usr/share/applications/exo-mail-reader.desktop
Path=
